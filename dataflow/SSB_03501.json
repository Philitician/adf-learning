{
	"name": "SSB_03501",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "InputBlob",
						"type": "LinkedServiceReference"
					},
					"name": "SsbBlobSource"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "OutputSql",
						"type": "LinkedServiceReference"
					},
					"name": "SQLOutput"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          region as string,",
				"          {årsak} as string,",
				"          dyr as string,",
				"          {Registrert avgang av hjortevilt 1987-1988} as integer,",
				"          {Registrert avgang av hjortevilt 1988-1989} as integer,",
				"          {Registrert avgang av hjortevilt 1989-1990} as integer,",
				"          {Registrert avgang av hjortevilt 2020-2021} as integer,",
				"          {Registrert avgang av hjortevilt 2021-2022} as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     container: 'ssbinput',",
				"     folderPath: '03501',",
				"     fileName: '03501.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> SsbBlobSource",
				"SsbBlobSource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          region as string,",
				"          {årsak} as string,",
				"          dyr as string,",
				"          avgang_1987_til_1988 as decimal(18,0),",
				"          avgang_1988_til_1989 as decimal(18,0),",
				"          avgang_1989_til_1990 as decimal(18,0),",
				"          avgang_2020_til_2021 as decimal(18,0),",
				"          avgang_2021_til_2022 as decimal(18,0)",
				"     ),",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'dbo',",
				"     tableName: 'ssb_03501',",
				"     insertable: true,",
				"     updateable: false,",
				"     deletable: false,",
				"     upsertable: false,",
				"     recreate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          region,",
				"          {årsak},",
				"          dyr,",
				"          avgang_1987_til_1988 = {Registrert avgang av hjortevilt 1987-1988},",
				"          avgang_1988_til_1989 = {Registrert avgang av hjortevilt 1988-1989},",
				"          avgang_1989_til_1990 = {Registrert avgang av hjortevilt 1989-1990},",
				"          avgang_2020_til_2021 = {Registrert avgang av hjortevilt 2020-2021},",
				"          avgang_2021_til_2022 = {Registrert avgang av hjortevilt 2020-2021}",
				"     )) ~> SQLOutput"
			]
		}
	}
}
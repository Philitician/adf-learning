					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "POST",
							"requestBody": "{\n  \"query\": [\n    {\n      \"code\": \"Region\",\n      \"selection\": {\n        \"filter\": \"item\",\n        \"values\": [\n          \"0\",\n          \"30\",\n          \"01\",\n          \"99\",\n          \"9999\"\n        ]\n      }\n    },\n    {\n      \"code\": \"Aarsak\",\n      \"selection\": {\n        \"filter\": \"item\",\n        \"values\": [\n          \"0\",\n          \"1\",\n          \"2\",\n          \"5\",\n          \"6\"\n        ]\n      }\n    },\n    {\n      \"code\": \"Dyr3\",\n      \"selection\": {\n        \"filter\": \"item\",\n        \"values\": [\n          \"1\",\n          \"2\",\n          \"3\",\n          \"4\"\n        ]\n      }\n    },\n    {\n      \"code\": \"ContentsCode\",\n      \"selection\": {\n        \"filter\": \"item\",\n        \"values\": [\n          \"AvgangHjort\"\n        ]\n      }\n    },\n    {\n      \"code\": \"Tid\",\n      \"selection\": {\n        \"filter\": \"item\",\n        \"values\": [\n          \"1987-1988\",\n          \"1988-1989\",\n          \"1989-1990\",\n          \"2020-2021\",\n          \"2021-2022\"\n        ]\n      }\n    }\n  ],\n  \"response\": {\n    \"format\": \"csv\"\n  }\n}",
